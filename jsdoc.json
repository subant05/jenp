{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["src/"],
        "exclude": ["node_modules", "__tests__","README.md","dist"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./docs/"
    },
    "docdash": {
        "static": [true],         
        "sort": [true],           
        "sectionOrder": [        
             "Classes",
             "Modules",
             "Externals",
             "Events",
             "Namespaces",
             "Mixins",
             "Tutorials",
             "Interfaces"
        ],
        "disqus": "",                   
        "openGraph": {                  
            "title": "Stack: functional library and framework building tools",                
            "type": "documentation",          
            "image": "",                
            "site_name": "Stack.js",            
            "url": "http://stackjs.com"
        },
        "meta": {                       
            "title": "",                
            "description": "",          
            "keyword": ""               
        },
        "search": [true],         
        "collapse": [true],       
        "wrap": [true],           
        "typedefs": [true],       
        "navLevel": [0],          
        "private": [false],        
        "removeQuotes": ["all"] ,
        "scripts": [],                   
        "menu":{                        
        }
    }
}